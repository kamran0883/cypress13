{
  "name": "lastestcypress",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "cypress run",
    "dev": "npx cypress open --env fileConfig=dev",
    "stage": "npx cypress open --env fileConfig=stage",
    "prod": "npx cypress open --env fileConfig=dev",
    "TestbackendFiles": "npx cypress run --spec cypress/e2e/**/*.feature",
    "test:chrome": "cypress run --browser chrome",
    "test:firefox": "cypress run --browser firefox",
    "test:electron": "cypress run --browser electron",
    "test:webkit": "cypress run --browser webkit",
    "test:all": "npm run test:chrome && npm run test:firefox && npm run test:electron"
  },
  "html": {
    "enabled": true,
    "output": "cypress/reports/json/cucumber-report.html",
    "formatter": "cucumber-json-formatter.exe"
  },
  "filterSpecs": true,
  "omitFiltered": true,
  "cypress-cucumber-preprocessor": {
    "stepDefinitions": [
      "cypress/e2e/step_definitions/**/*.js"
    ]
  },
  "author": "Muhammad Inzamam Akbar",
  "license": "ISC",
  "dependencies": {
    "cypress": "^13.6.1",
    "cypress-mailslurp": "^1.9.0",
    "cypress-mochawesome-reporter": "^3.7.0",
    "moment": "^2.30.1",
    "yarn": "^1.22.21"
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^20.0.1",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.0",
    "@cypress/grep": "^4.0.1",
    "cypress-xpath": "^2.0.1",
    "mocha": "^10.2.0",
    "mocha-foo-reporter": "^0.0.1",
    "mochawesome": "^7.1.3",
    "playwright-webkit": "1.34"
  }
}
